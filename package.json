{
	"name": "magpiefi-challenge",
	"version": "1.0.0",
	"description": "",
	"main": "dist/index.js",
	"type": "module",
	"engines": {
		"node": ">=19.x",
		"npm": ">=9.x"
	},
	"scripts": {
		"build": "tsc",
		"build:esbuild": "etsc --config etsc.config.js",
		"start": "node --trace-warnings --trace-deprecation --experimental-modules --es-module-specifier-resolution=node dist",
		"dev": "tsc-watch --onSuccess 'node --trace-warnings --trace-deprecation --experimental-modules --es-module-specifier-resolution=node dist'",
		"dev:esbuild": "nodemon",
		"lint": "eslint --ext .ts,.js . --ignore-path .gitignore",
		"fix": "eslint --ext .ts,.js . --ignore-path .gitignore --fix",
		"lint-staged": "lint-staged",
		"test": "vitest dev",
		"test:unit": "vitest run -t plugins?",
		"test:ci": "CI=true vitest run",
		"test:coverage": "vitest run --coverage",
		"push": "prisma db push --skip-generate",
		"pull": "prisma db pull",
		"seed": "prisma db seed",
		"generate": "prisma generate",
		"postinstall": "husky install"
	},
	"keywords": [],
	"author": "",
	"license": "ISC",
	"devDependencies": {
		"@types/node": "^18.14.2",
		"@types/qs": "^6.9.7",
		"@types/uuid": "^9.0.1",
		"@typescript-eslint/eslint-plugin": "^5.54.0",
		"@typescript-eslint/parser": "^5.54.0",
		"@vitest/coverage-c8": "^0.29.1",
		"cpy": "^9.0.1",
		"esbuild": "^0.17.10",
		"esbuild-node-tsc": "^2.0.5",
		"eslint": "^8.35.0",
		"eslint-config-prettier": "^8.6.0",
		"eslint-plugin-prettier": "^4.2.1",
		"eslint-plugin-security": "^1.7.1",
		"husky": "^8.0.3",
		"lint-staged": "^13.1.2",
		"nodemon": "^2.0.20",
		"prettier": "^2.8.4",
		"prisma": "^4.10.1",
		"rimraf": "^4.1.2",
		"ts-node": "^10.9.1",
		"tsc-watch": "^6.0.0",
		"typescript": "^4.9.5",
		"vitest": "^0.29.1"
	},
	"dependencies": {
		"@fastify/env": "^4.2.0",
		"@fastify/sensible": "^5.2.0",
		"@fastify/swagger": "^8.3.1",
		"@fastify/swagger-ui": "^1.4.0",
		"@fastify/under-pressure": "^8.2.0",
		"@prisma/client": "^4.10.1",
		"@total-typescript/ts-reset": "^0.3.7",
		"ajv": "^8.12.0",
		"ajv-errors": "^3.0.0",
		"ajv-formats": "^2.1.1",
		"fastify": "^4.13.0",
		"fastify-cron": "^1.3.1",
		"fastify-plugin": "^4.5.0",
		"fluent-json-schema": "^4.1.0",
		"graphql": "^16.6.0",
		"make-promises-safe": "^5.1.0",
		"pino": "^8.11.0",
		"pino-pretty": "^9.4.0",
		"qs": "^6.11.0",
		"urql": "^3.0.3",
		"uuid": "^9.0.0"
	},
	"lint-staged": {
		"*.{js,ts,json,md,yml,yaml,graphql,gql}": [
			"prettier --write"
		]
	}
}
